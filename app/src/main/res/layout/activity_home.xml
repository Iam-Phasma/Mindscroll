<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Home">

    <TextView
        android:id="@+id/tvNameHome"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:fontFamily="@font/calibribold"
        android:text="Username"
        android:textColor="#000000"
        android:textSize="24sp"
        android:textStyle="bold"
        android:visibility="invisible"
        app:layout_constraintEnd_toEndOf="@+id/imageView4"
        app:layout_constraintStart_toStartOf="@+id/imageView4"
        app:layout_constraintTop_toBottomOf="@+id/imageView4">

    </TextView>

    <TextView
        android:id="@+id/tvDate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:fontFamily="@font/calibriregular"
        android:text="Day, Month Day Year"
        android:textColor="#000000"
        android:textSize="15sp"
        app:layout_constraintEnd_toEndOf="@+id/imageView4"
        app:layout_constraintHorizontal_bias="0.588"
        app:layout_constraintStart_toStartOf="@+id/imageView4"
        app:layout_constraintTop_toBottomOf="@+id/etNameHome">

    </TextView>

    <TextView
        android:id="@+id/tvnote"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="-10dp"
        android:elevation="10dp"
        android:fontFamily="@font/calibribold"
        android:text="Try health activities"
        android:textColor="#FFFFFF"
        android:textSize="12sp"
        app:layout_constraintBottom_toTopOf="@+id/imgbtnBoardLook"
        app:layout_constraintEnd_toEndOf="@+id/imgbtnBoardLook"
        app:layout_constraintHorizontal_bias="0.87"
        app:layout_constraintStart_toStartOf="@+id/imgbtnBoardLook">

    </TextView>

    <TextView
        android:id="@+id/tvnote2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="-10dp"
        android:elevation="10dp"
        android:fontFamily="@font/calibribold"
        android:text="Today's phone usage"
        android:textColor="#FFFFFF"
        android:textSize="12sp"
        app:layout_constraintBottom_toTopOf="@+id/imgbtnBoardCoffee"
        app:layout_constraintEnd_toEndOf="@+id/imgbtnBoardCoffee"
        app:layout_constraintHorizontal_bias="0.89"
        app:layout_constraintStart_toStartOf="@+id/imgbtnBoardCoffee">

    </TextView>

    <ImageButton
        android:id="@+id/imgbtnBoardCoffee"
        android:layout_width="357dp"
        android:layout_height="116dp"
        android:layout_marginBottom="216dp"
        android:backgroundTint="#00FFFFFF"
        android:clickable="true"
        android:contentDescription="realtimeboards"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.472"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/gif_board_3_noshadow" />

    <ImageButton
        android:id="@+id/imgbtnBoardMusic"
        android:layout_width="357dp"
        android:layout_height="116dp"
        android:backgroundTint="#00FFFFFF"
        android:clickable="true"
        android:contentDescription="realtimeboards"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/gif_board_3_noshadow" />

    <ImageButton
        android:id="@+id/imgbtnBoardLook"
        android:layout_width="357dp"
        android:layout_height="116dp"
        android:layout_marginBottom="88dp"
        android:backgroundTint="#00FFFFFF"
        android:clickable="true"
        android:contentDescription="realtimeboards"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.489"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/gif_board_3_noshadow" />

    <ImageButton
        android:id="@+id/btnAppInfo"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:backgroundTint="#00FFFFFF"
        android:contentDescription="menubutton"
        android:scaleType="fitCenter"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/menu_dots" />

    <ImageButton
        android:id="@+id/btnTrivia"
        android:layout_width="97dp"
        android:layout_height="53dp"
        android:layout_marginEnd="8dp"
        android:backgroundTint="#00FFFFFF"
        android:contentDescription="triviabutton"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="@+id/btnAppInfo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/btnAppInfo"
        app:layout_constraintVertical_bias="0.571"
        app:srcCompat="@drawable/home_trivia" />

    <Button

        android:id="@+id/btnViewDetails"
        android:layout_width="110dp"
        android:layout_height="47dp"
        android:layout_marginTop="20dp"
        android:backgroundTint="#0FCF55"
        android:contentDescription="prevstatsbutton"
        android:paddingLeft="0dp"
        android:paddingRight="0dp"
        android:text="View Details"
        android:textColor="#FFFFFF"
        android:textSize="10sp"
        app:layout_constraintEnd_toEndOf="@+id/imageView4"
        app:layout_constraintHorizontal_bias="0.488"
        app:layout_constraintStart_toStartOf="@+id/imageView4"
        app:layout_constraintTop_toBottomOf="@+id/tvDate" />

    <TextView
        android:id="@+id/tvScreenTimeHoursHome"
        android:layout_width="77dp"
        android:layout_height="wrap_content"
        android:fontFamily="@font/calibribold"
        android:paddingLeft="0dp"
        android:paddingTop="0dp"
        android:paddingRight="0dp"
        android:paddingBottom="0dp"
        android:text="00"
        android:textAlignment="center"
        android:textColor="#7C7C7C"
        android:textSize="14sp"
        android:textStyle="bold"
        android:typeface="sans"
        app:layout_constraintBottom_toBottomOf="@+id/tvScreenWakeCountHome"
        app:layout_constraintEnd_toEndOf="@+id/textView4"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView4"
        app:layout_constraintTop_toTopOf="@+id/tvScreenWakeCountHome"
        app:layout_constraintVertical_bias="0.13">

    </TextView>

    <TextView
        android:id="@+id/tvScreenWakeCountHome"
        android:layout_width="77dp"
        android:layout_height="wrap_content"
        android:fontFamily="@font/calibribold"
        android:paddingLeft="0dp"
        android:paddingTop="0dp"
        android:paddingRight="0dp"
        android:paddingBottom="0dp"
        android:text="00"
        android:textAlignment="center"
        android:textColor="#7C7C7C"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/imgbtnBoardCoffee"
        app:layout_constraintEnd_toEndOf="@+id/textView2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView2"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        app:layout_constraintVertical_bias="0.0">

    </TextView>

    <TextView
        android:id="@+id/textView2"
        android:layout_width="77dp"
        android:layout_height="31dp"
        android:fontFamily="@font/calibribold"
        android:text="Screen Wake"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/imgbtnBoardCoffee"
        app:layout_constraintEnd_toEndOf="@+id/imgbtnBoardCoffee"
        app:layout_constraintHorizontal_bias="0.389"
        app:layout_constraintStart_toStartOf="@+id/imgbtnBoardCoffee"
        app:layout_constraintTop_toTopOf="@+id/imgbtnBoardCoffee"
        app:layout_constraintVertical_bias="0.382">

    </TextView>

    <TextView
        android:id="@+id/textView"
        android:layout_width="210dp"
        android:layout_height="31dp"
        android:fontFamily="@font/calibribold"
        android:text="Look far before you go to sleep after a long use of your phone"
        android:textAlignment="textStart"
        android:textColor="#000000"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/imgbtnBoardLook"
        app:layout_constraintEnd_toEndOf="@+id/imgbtnBoardLook"
        app:layout_constraintHorizontal_bias="0.76"
        app:layout_constraintStart_toEndOf="@+id/imgviewLookFar"
        app:layout_constraintStart_toStartOf="@+id/imgbtnBoardLook"
        app:layout_constraintTop_toTopOf="@+id/imgbtnBoardLook"
        app:layout_constraintVertical_bias="0.37">

    </TextView>

    <TextView
        android:id="@+id/textView7"
        android:layout_width="210dp"
        android:layout_height="31dp"
        android:layout_marginTop="31dp"
        android:layout_marginBottom="46dp"
        android:fontFamily="@font/calibribold"
        android:text="Listen to ambience music to help you relax and sleep"
        android:textAlignment="textStart"
        android:textColor="#000000"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/imgbtnBoardMusic"
        app:layout_constraintEnd_toEndOf="@+id/imgbtnBoardMusic"
        app:layout_constraintHorizontal_bias="0.761"
        app:layout_constraintStart_toStartOf="@+id/imgbtnBoardMusic"
        app:layout_constraintTop_toTopOf="@+id/imgbtnBoardMusic"
        app:layout_constraintVertical_bias="0.0">

    </TextView>

    <TextView
        android:id="@+id/textView5"
        android:layout_width="200dp"
        android:layout_height="31dp"
        android:fontFamily="@font/calibribold"
        android:text="Reduce and prevent digital eye strain"
        android:textAlignment="textStart"
        android:textColor="#7C7C7C"
        android:textSize="10sp"
        app:layout_constraintBottom_toBottomOf="@+id/imgbtnBoardLook"
        app:layout_constraintStart_toStartOf="@+id/textView"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:layout_constraintVertical_bias="0.31">

    </TextView>

    <TextView
        android:id="@+id/textView9"
        android:layout_width="200dp"
        android:layout_height="31dp"
        android:layout_marginTop="31dp"
        android:fontFamily="@font/calibribold"
        android:text="Mix and match sounds to suit your day"
        android:textAlignment="textStart"
        android:textColor="#7C7C7C"
        android:textSize="10sp"
        app:layout_constraintBottom_toBottomOf="@+id/imgbtnBoardMusic"
        app:layout_constraintStart_toStartOf="@+id/textView7"
        app:layout_constraintTop_toTopOf="@+id/textView7"
        app:layout_constraintVertical_bias="0.37">

    </TextView>

    <TextView
        android:id="@+id/textView4"
        android:layout_width="77dp"
        android:layout_height="31dp"
        android:fontFamily="@font/calibribold"
        android:text="Active Hours"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/textView2"
        app:layout_constraintEnd_toEndOf="@+id/imgbtnBoardCoffee"
        app:layout_constraintHorizontal_bias="0.81"
        app:layout_constraintStart_toStartOf="@+id/imgbtnBoardCoffee"
        app:layout_constraintTop_toTopOf="@+id/textView2"
        app:layout_constraintVertical_bias="0.018">

    </TextView>

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="155dp"
        android:layout_height="155dp"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnTrivia"
        app:srcCompat="@drawable/home_profileshadow">

    </ImageView>

    <ImageView
        android:id="@+id/imgviewProfile"
        android:layout_width="145dp"
        android:layout_height="145dp"
        android:clickable="true"
        android:elevation="100dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView4"
        app:layout_constraintEnd_toEndOf="@+id/imageView4"
        app:layout_constraintHorizontal_bias="0.466"
        app:layout_constraintStart_toStartOf="@+id/imageView4"
        app:layout_constraintTop_toTopOf="@+id/imageView4"
        app:layout_constraintVertical_bias="0.466"
        app:srcCompat="@drawable/home_startup_profile">

    </ImageView>

    <ImageView
        android:id="@+id/imgviewCoffee"
        android:layout_width="55dp"
        android:layout_height="48dp"
        app:layout_constraintBottom_toBottomOf="@+id/imgbtnBoardCoffee"
        app:layout_constraintEnd_toEndOf="@+id/imgbtnBoardCoffee"
        app:layout_constraintHorizontal_bias="0.103"
        app:layout_constraintStart_toStartOf="@+id/imgbtnBoardCoffee"
        app:layout_constraintTop_toTopOf="@+id/imgbtnBoardCoffee"
        app:layout_constraintVertical_bias="0.467"
        app:srcCompat="@drawable/coffee" />

    <ImageView
        android:id="@+id/imgviewLookFar"
        android:layout_width="55dp"
        android:layout_height="48dp"
        app:layout_constraintBottom_toBottomOf="@+id/imgbtnBoardLook"
        app:layout_constraintEnd_toEndOf="@+id/imgbtnBoardLook"
        app:layout_constraintHorizontal_bias="0.103"
        app:layout_constraintStart_toStartOf="@+id/imgbtnBoardLook"
        app:layout_constraintTop_toTopOf="@+id/imgbtnBoardLook"
        app:layout_constraintVertical_bias="0.483"
        app:srcCompat="@drawable/look" />

    <ImageView
        android:id="@+id/imgviewMusic"
        android:layout_width="55dp"
        android:layout_height="48dp"
        android:layout_marginTop="31dp"
        android:layout_marginBottom="29dp"
        app:layout_constraintBottom_toBottomOf="@+id/imgbtnBoardMusic"
        app:layout_constraintEnd_toEndOf="@+id/imgbtnBoardMusic"
        app:layout_constraintHorizontal_bias="0.103"
        app:layout_constraintStart_toStartOf="@+id/imgbtnBoardMusic"
        app:layout_constraintTop_toTopOf="@+id/imgbtnBoardMusic"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/notes" />

    <EditText
        android:id="@+id/etNameHome"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:background="#00FFFFFF"
        android:clickable="true"
        android:ems="10"
        android:focusable="true"
        android:focusableInTouchMode="true"
        android:fontFamily="@font/calibribold"
        android:hint="Enter Username"
        android:imeOptions="actionUnspecified"
        android:inputType="textPersonName"
        android:maxLength="10"
        android:selectAllOnFocus="true"
        android:singleLine="true"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textColorHint="#808080"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="@+id/imgviewProfile"
        app:layout_constraintHorizontal_bias="0.482"
        app:layout_constraintStart_toStartOf="@+id/imageView4"
        app:layout_constraintTop_toBottomOf="@+id/imageView4" />

</androidx.constraintlayout.widget.ConstraintLayout>